<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="one.hust.edu.cn.dao.DataAuthorityDao">
    <resultMap id="BaseResultMap" type="one.hust.edu.cn.entities.DataAuthority">
        <!--映射column为数据库字段，property为实体类属性-->
        <id column="id" property="id" jdbcType="INTEGER"/>
        <id column="user_id" property="userId" jdbcType="INTEGER"/>
        <id column="data_sample_id" property="dataSampleId" jdbcType="INTEGER"/>
        <id column="authority_key" property="authorityKey" jdbcType="INTEGER"/>
    </resultMap>
    <insert id="addDataAuthority" parameterType="DataAuthority" useGeneratedKeys="true" keyProperty="id">
        insert into data_authority (user_id,data_sample_id,authority_key) values (#{userId},#{dataSampleId},#{authorityKey});
    </insert>
</mapper>